<Window x:Class="SMI1002_TP1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SMI1002_TP1"
        mc:Ignorable="d"
        Title="MainWindow" Height="650" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button Content="Réservation" MinWidth="100" Click="BtnClickPreservation"/>
            <Button Content="Annulation" MinWidth="100" Margin="05,0,0,0" Click="BtnClickPannulation"/>
            <Button Content="Client" MinWidth="100" Margin="05,0,0,0" Click="BtnClickPclient"/>
            <Button Content="Historique client" MinWidth="100" Margin="05,0,0,0" Click="BtnClickPhistorique"/>
        </StackPanel>
        <Frame x:Name="Main" Margin="0,35,0,0"/>
        
    </Grid>
</Window>
